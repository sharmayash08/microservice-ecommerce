{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "description": {
      "type": "text"
    },
    "inStock": {
      "type": "boolean"
    },
    "reviewCount": {
      "type": "integer"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Top",
        "Bottom",
        "Dress",
        "Jewellery",
        "Cosmetics"
      ]
    },
    "isBestseller": {
      "type": "boolean"
    },
    "rating": {
      "type": "integer"
    }
  }
}
